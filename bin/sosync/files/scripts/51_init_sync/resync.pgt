INSERT INTO sosync_sync_table(odoo_id#{id2_sync_table_field:, }, table_name, creation, operation)
SELECT #{id_field_name}#{id2_field_name:, }, '#{table_name}', now()
	, case when #{id_field_name} in
		(
		select 
			 odoo_id
		from sosync_sync_table t
		where t.table_name = '#{table_name}'
		  and t.operation = 'i'
		) 
		or #{id_field_name} in
		(
		select  
			 #{id_field_name}
		from #{table_name} b
		where b.create_uid = #{odooUserID}
		)
		then 'u' else 'i' end
FROM #{table_name};