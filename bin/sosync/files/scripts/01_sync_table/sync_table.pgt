CREATE TABLE IF NOT EXISTS sosync_sync_table
(
  id serial NOT NULL,
  table_name character varying(128),
  odoo_id integer,
  odoo_id2 integer,
  operation character(1),
  creation timestamp without time zone,
  fetched integer,
  CONSTRAINT odooSync_pk_sync_table PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);