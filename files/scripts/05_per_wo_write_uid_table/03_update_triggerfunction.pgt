CREATE OR REPLACE FUNCTION sosync_triggerfunction_update_#{table_name}() RETURNS trigger AS $$
        DECLARE
        BEGIN

                INSERT INTO sosync_sync_table(odoo_id#{id2_sync_table_field:, }, table_name, creation, operation)
                SELECT new.#{id_field_name}#{id2_field_name:, new.}, '#{table_name}', now(), 'u';

		NOTIFY #{notification_channel};

                RETURN NULL;
        END;
$$ LANGUAGE plpgsql;