create or replace view sosync_current_odoo_user_id
as
select 
	replace(
			substr(
				   prosrc
				  ,strpos(
						  prosrc
						 ,'new.write_uid = '
						 ) + 16 
				  ,strpos(
						  substr(
								 prosrc
								,strpos(
										prosrc
									   ,'new.write_uid = ') + 16
									   )
								,'THEN RETURN NEW'
								) - 1
						 )
		   ,' '
		   ,''
		   ) sosync_current_odoo_user_id 
from pg_proc 
where proname= 'sosync_triggerfunction_insert_product_product';